{
    "name": "tobento/service-country",
    "type": "library",
    "description": "Country interfaces and resources for PHP applications.",
    "keywords": [
        "tobento",
        "package",
        "PHP",
        "country",
        "countries"
    ],
    "homepage": "https://www.tobento.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Strub",
            "email": "info@tobento.ch",
            "homepage": "https://www.tobento.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "tobento/service-collection": "^1.0",
        "tobento/service-support": "^1.0"
    },  
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Tobento\\Service\\Country\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tobento\\Service\\Country\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always",
        "psalm": "vendor/bin/psalm"
    }
}